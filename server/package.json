{
  "name": "sdo-rssu-reload-server",
  "version": "0.0.2",
  "main": "build/index.js",
  "author": "Nick Chaykovsky <lis@chaikovskie.com>",
  "private": true,
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.{ts,js}'",
    "start": "pnpm build && node build/index.js",
    "dev": "nodemon --watch src -e ts --exec ts-node src/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "@types/uuid": "^3.4.7",
    "axios": "^0.24.0",
    "fastify": "^2.15.1",
    "fastify-cors": "^3.0.1",
    "fastify-helmet": "^3.0.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "pretty-quick",
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
